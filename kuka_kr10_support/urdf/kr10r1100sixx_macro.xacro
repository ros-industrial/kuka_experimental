<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:include filename="$(find kuka_resources)/urdf/common_constants.xacro"/>
  <xacro:include filename="$(find kuka_resources)/urdf/common_materials.xacro"/>

  <!-- Mass parameters (guessed), total mass should be 55 kg -->
  <xacro:property name="kuka_kr10r1100sixx_base_link_mass" value="4.0" />
  <xacro:property name="kuka_kr10r1100sixx_link_1_mass" value="4.0" />
  <xacro:property name="kuka_kr10r1100sixx_link_2_mass" value="19.0" />
  <xacro:property name="kuka_kr10r1100sixx_link_3_mass" value="10.0" />
  <xacro:property name="kuka_kr10r1100sixx_link_4_mass" value="15.0" />
  <xacro:property name="kuka_kr10r1100sixx_link_5_mass" value="2.8" />
  <xacro:property name="kuka_kr10r1100sixx_link_6_mass" value="0.2" />

  <!-- Kinematic parameters -->
  <xacro:property name="kuka_kr10r1100sixx_d1" value="0.400" />
  <xacro:property name="kuka_kr10r1100sixx_d4" value="0.515" />
  <xacro:property name="kuka_kr10r1100sixx_a1" value="0.025" />
  <xacro:property name="kuka_kr10r1100sixx_a2" value="0.560" />
  <xacro:property name="kuka_kr10r1100sixx_a3" value="0.035" />
  <xacro:property name="kuka_kr10r1100sixx_a5" value="0.080" />

  <!-- Cylindrical inertia tensor -->
  <xacro:macro name="cylinder_inertial" params="radius length mass *origin">
   <inertial>
     <mass value="${mass}" />
     <xacro:insert_block name="origin" />
     <inertia ixx="${0.0833333 * mass * (3 * radius * radius + length * length)}" ixy="0.0" ixz="0.0"
       iyy="${0.0833333 * mass * (3 * radius * radius + length * length)}" iyz="0.0"
       izz="${0.5 * mass * radius * radius}" />
   </inertial>
 </xacro:macro>


  <xacro:macro name="kuka_kr10r1100sixx" params="prefix">
    <link name="${prefix}base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/base_link.stl" />
        </geometry>
        <xacro:material_kuka_black />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/base_link.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_d1/4}" length="${kuka_kr10r1100sixx_d1/2}" mass="${kuka_kr10r1100sixx_base_link_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_1.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_1.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_d1/4}" length="${kuka_kr10r1100sixx_d1/2}" mass="${kuka_kr10r1100sixx_link_1_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_2.stl"/>
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_2.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_d1/4}" length="${kuka_kr10r1100sixx_a2}" mass="${kuka_kr10r1100sixx_link_2_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_3">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_3.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_3.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_a3}" length="${kuka_kr10r1100sixx_a3*10}" mass="${kuka_kr10r1100sixx_link_3_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_4.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_4.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_d1/4}" length="${kuka_kr10r1100sixx_d4}" mass="${kuka_kr10r1100sixx_link_4_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_5">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_5.stl" />
        </geometry>
        <xacro:material_kuka_orange />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_5.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_a5/2}" length="${kuka_kr10r1100sixx_a5}" mass="${kuka_kr10r1100sixx_link_5_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/visual/link_6.stl" />
        </geometry>
        <xacro:material_kuka_pedestal />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://kuka_kr10_support/meshes/kr10r1100sixx/collision/link_6.stl" />
        </geometry>
      </collision>
      <xacro:cylinder_inertial radius="${kuka_kr10r1100sixx_a5/2}" length="${kuka_kr10r1100sixx_a5/10}" mass="${kuka_kr10r1100sixx_link_6_mass}">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <!-- This frame corresponds to the $FLANGE coordinate system in KUKA KRC controllers. -->
    <link name="${prefix}tool0"/>

    <joint name="${prefix}joint_a1" type="revolute">
      <origin xyz="0 0 ${kuka_kr10r1100sixx_d1}" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <axis xyz="0 0 -1"/>
      <limit effort="0" lower="${-DEG2RAD * 170}" upper="${DEG2RAD * 170}" velocity="${DEG2RAD * 360}"/>
    </joint>
    <joint name="${prefix}joint_a2" type="revolute">
      <origin xyz="${kuka_kr10r1100sixx_a1} 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 190}" upper="${DEG2RAD * 45}" velocity="${DEG2RAD * 300}"/>
    </joint>
    <joint name="${prefix}joint_a3" type="revolute">
      <origin xyz="${kuka_kr10r1100sixx_a2} 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 120}" upper="${DEG2RAD * 156}" velocity="${DEG2RAD * 360}"/>
    </joint>
    <joint name="${prefix}joint_a4" type="revolute">
      <origin xyz="0 0 ${kuka_kr10r1100sixx_a3}" rpy="0 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-DEG2RAD * 185}" upper="${DEG2RAD * 185}" velocity="${DEG2RAD * 381}"/>
    </joint>
    <joint name="${prefix}joint_a5" type="revolute">
      <origin xyz="${kuka_kr10r1100sixx_d4} 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <axis xyz="0 1 0"/>
      <limit effort="0" lower="${-DEG2RAD * 120}" upper="${DEG2RAD * 120}" velocity="${DEG2RAD * 388}"/>
    </joint>
    <joint name="${prefix}joint_a6" type="revolute">
      <origin xyz="${kuka_kr10r1100sixx_a5} 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <axis xyz="-1 0 0"/>
      <limit effort="0" lower="${-DEG2RAD * 350}" upper="${DEG2RAD * 350}" velocity="${DEG2RAD * 615}"/>
    </joint>
    <joint name="${prefix}joint_a6-tool0" type="fixed">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}tool0"/>
      <origin xyz="0 0 0" rpy="0 ${DEG2RAD * 90} 0"/>
    </joint>

    <!-- ROS base_link to KUKA $ROBROOT coordinate system transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
  </xacro:macro>
</robot>
